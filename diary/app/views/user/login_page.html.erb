<h3>please login</h3>
<span style="color:red"><%= flash[:msg]%></span>
<%= render(:partial => "shared/error_messages", :locals => {:object => @user})%>
<%= form_tag(:action => "attemp_login") do%>
	<%= label_tag(:name)%>:
	<%= text_field_tag(:name)%><br>
	<%= label_tag(:password)%>:
	<%= password_field_tag(:password)%><br>
	<%= submit_tag("log in")%>
<%end%>
<%= link_to('register',:action => 'register')%>
